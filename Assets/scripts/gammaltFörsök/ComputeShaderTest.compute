#pragma kernel CSMain

struct Cube
{
    float3 pos;
    float4 color;
};

RWStructuredBuffer<Cube> cubes;

[numthreads(10, 1, 1)]
void CSMain (uint3 id : SV_DispatchThreadID)
{

    float xPos = id.x / resolution;

    Cube c = cubes[id.x];
    c.color = float4(xPos, 0, 0, 1.0);

    cubes[id.x] = c;

}
